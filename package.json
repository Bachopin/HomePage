{
  "name": "notion-home",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3456 -H 0.0.0.0",
    "build": "npm run optimize-images && next build",
    "build:fast": "next build",
    "start": "next start -p 3456",
    "lint": "next lint",
    "restart": "sh scripts/restart.sh",
    "quick-restart": "sh scripts/quick-restart.sh",
    "optimize-images": "node scripts/optimize-images.js",
    "optimize-images:force": "rm -rf public/images/optimized && npm run optimize-images",
    "test-image-quality": "node scripts/test-image-quality.js",
    "health-check": "node scripts/health-check.js",
    "deploy-check": "npm run health-check && npm run build:fast"
  },
  "dependencies": {
    "@notionhq/client": "^5.6.0",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.4",
    "framer-motion": "^11.3.19",
    "lucide-react": "^0.562.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/typed.js": "^2.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  }
