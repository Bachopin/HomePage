{
  "name": "notion-home",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001 -H 0.0.0.0",
    "dev:restart": "./scripts/dev-restart.sh",
    "dev:start": "./scripts/dev-start.sh",
    "dev:clean": "rm -rf .next node_modules/.cache && npm cache clean --force",
    "dev:kill": "lsof -ti:3001 | xargs kill -9 2>/dev/null || echo 'No process on port 3001'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@notionhq/client": "^5.6.0",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.4",
    "framer-motion": "^11.3.19",
    "lucide-react": "^0.562.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/typed.js": "^2.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  }
}
